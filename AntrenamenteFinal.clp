(defrule R1
=>
(open intrare.dat fdate "r")
(open iesire.txt fiesire "w")
(assert (faza citire) ) 
)


(defrule R2
?a <- (faza citire)
=>
(retract ?a)
(assert (valoare-citita (readline fdate))) 
)


(defrule R3
"regula ce actioneaza atunci cand valoarea citita din fisier este EOF"
?a <-(valoare-citita EOF)
=>
(retract ?a)
(assert (faza oprire))
)


(defrule R4
?a <- (faza oprire)
=>
(retract ?a)
)


(defrule R5
?a <- (valoare-citita ?v)
=>
(retract ?a)
(str-assert(str-cat "( " ?v ")" ))
(assert (faza citire)) 
)


(defrule R6 (declare(salience -1))
(informatie gen masculin)
(informatie nivelDeActivitate ?nivelActivitate)
=>
(if(eq ?nivelActivitate sedentar)
	then ( assert (split Full_Body) (frecventa 3 zile) (pauza 4 zile))
)

(if(eq ?nivelActivitate normal)
	then ( assert (split Upper_Lower) (frecventa 4 zile) (pauza 3 zile))
)

(if(eq ?nivelActivitate activ)
	then ( assert (split Bro_Split) (frecventa 5 zile) (pauza 2 zile))
)

(if(eq ?nivelActivitate foarte_activ)
	then ( assert (split Push_Pull_Legs) (frecventa 6 zile) (pauza 1 zile))
)
)


(defrule R7 (declare (salience -1))
(informatie bmi ?bmi)
=>
(if(< ?bmi 18.5)
	then ( assert (persoana subponderal))
)

(if (and (>= ?bmi 18.5) (< ?bmi 24.9))
	then ( assert (persoana normal))
)

(if (and (>= ?bmi 25) (< ?bmi 29.9))
	then ( assert (persoana supraponderal))
)

(if(>= ?bmi 30)
	then ( assert (persoana obez))
)
)


(defrule R8 (declare (salience -2))
(split Full_Body)
(frecventa 3 zile)
(pauza 4 zile)
=>
(assert
(Ziua 1 Luni back chest biceps triceps shoulders legs)
(Ziua 2 Marti rest)
(Ziua 3 Miercuri back chest biceps triceps shoulders legs abdomen) 
(Ziua 4 Joi rest)
(Ziua 5 Vineri back chest biceps triceps shoulders legs)
(Ziua 6 Sambata rest)
(Ziua 7 Duminica rest)
)
)


(defrule R9 (declare(salience -2))
(split Upper_Lower) 
(frecventa 4 zile)
(pauza 3 zile)
=>
(assert
(Ziua 1 Luni back biceps chest triceps shoulders) 
(Ziua 2 Marti legs legs legs legs legs abdomen) 
(Ziua 3 Miercuri rest)
(Ziua 4 Joi back biceps chest triceps shoulders) 
(Ziua 5 Vineri legs legs legs legs legs abdomen)
(Ziua 6 Sambata rest)
(Ziua 7 Duminica rest)
)
)


(defrule R10 (declare(salience -2))
(split Bro_Split) 
(frecventa 5 zile) 
(pauza 2 zile)
=>
(assert
(Ziua 1 Luni back back back back back abdomen abdomen) 
(Ziua 2 Marti chest chest chest chest chest) 
(Ziua 3 Miercuri shoulders shoulders shoulders shoulders shoulders abdomen abdomen) 
(Ziua 4 Joi legs legs legs legs legs) 
(Ziua 5 Vineri biceps biceps biceps triceps triceps triceps abdomen abdomen)
(Ziua 6 Sambata rest)
(Ziua 7 Duminica rest)
)
)


(defrule R11 (declare(salience -2))
(split Push_Pull_Legs) 
(frecventa 6 zile) 
(pauza 1 zile)
=>
(assert
(Ziua 1 Luni back back back back biceps biceps biceps biceps abdomen abdomen abdomen abdomen) 
(Ziua 2 Marti chest chest chest chest triceps triceps shoulders shoulders) 
(Ziua 3 Miercuri legs legs legs legs legs abdomen abdomen abdomen abdomen) 
(Ziua 4 Joi rest)
(Ziua 5 Vineri back back back back biceps biceps biceps biceps) 
(Ziua 6 Sambata chest chest triceps triceps triceps shoulders shoulders shoulders abdomen abdomen abdomen abdomen) 
(Ziua 7 Duminica legs legs legs legs legs)
)
)


(defrule R12 (declare(salience -3))
?a <- (Ziua ?numar ?zi ?grupa $?rest)
(exercitiu ?grupa ?nume ?seturi ?repetariMinime ?repetariMaxime $? incepator $?)
(informatie nivelDeActivitate sedentar)
=>
(retract ?a)
(assert (Ziua ?numar ?zi $?rest) (zi_antrenament ?zi ?nume ?seturi ?repetariMinime ?repetariMaxime) )
)


