(defrule R16(declare(salience -4))
(informatie nivelDeActivitate ?nivelActivitate)
(persoana ?tipPersoana)
=>
(if  (and (eq ?nivelActivitate sedentar) (or (eq ?tipPersoana obez) (eq ?tipPersoana supraponderal)))
 then (assert(rest 240-300))
 else (and (eq ?nivelActivitate sedentar) (eq ?tipPersoana normal))
 then (assert(rest 180-200))
 else (and (eq ?nivelActivitate sedentar) (eq ?tipPersoana subponderal))
 then (assert(rest 200-240))
)
)







(defrule R17 (declare(salience -7))
